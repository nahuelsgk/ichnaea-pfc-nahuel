{% extends '::GUI/withContainer.html.twig' %}

{% block page_title %}Ichnaea User List{% endblock %}


{% block main_content %}
<table class="table">
{% for user in users %}
<tr>
<td>{{ user.username }}</td>
<td>{{ user.email }}</td>
<td>
  <a class="btn btn-mini" href="{{ path('edit_user', {'user_id': user.id}) }}" name="edit_user" title="Add roles">
  <i class="icon-edit"></i>
</td>
</tr>
{% endfor %}
</table>
{% endblock %}